# Phase 3 Parallel Execution Configuration
# Set these environment variables to control parallel processing

# Enable/disable parallel processing (default: false for safety)
ENABLE_PARALLEL_PROCESSING=false

# Minimum complexity threshold for parallel processing (seconds)
PARALLEL_THRESHOLD=30

# Maximum parallel operations
MAX_PARALLEL_OPS=3

# Enable automatic fallback to legacy orchestrator on errors
PARALLEL_FALLBACK=true

# Parallel processing mode (development/production)
PARALLEL_MODE=development
